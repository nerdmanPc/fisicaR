pendulo.data <- read.csv(file="pendulo.csv",sep=";")
pendulo.data$tmedia <- (pendulo.data$t1 + pendulo.data$t2 + pendulo.data$t3)/15
pendulo.data$tmedia2 <- pendulo.data$tmedia * pendulo.data$tmedia
pendulo.massa <- 0.0671
pendulo.theta <- (5*pi)/180
pendulo.data$amplitude <- pendulo.data$comprimento * sin(theta)
pendulo.data$amplitude <- pendulo.data$comprimento * sin(pendulo.theta)
pendulo.data$omega <- (2*pi)/pendulo.data$tmedia
ls()
G <- 9.7833
pendulo.data
pendulo.Posicao <- function(t,i)
{
return(pendulo.data$amplitude[i]*cos(pendulo.data$omega[i]*t))
}
pendulo.Velocidade <- function(t,i)
{
return(-(pendulo.data$amplitude[i]*pendulo.data$omega[i]*sin(pendulo.data$omega[i]*t)))
}
pendulo.Aceleracao <- function(t,i)
{
return(-(pendulo.data$amplitude[i]*pendulo.data$omega[i]*pendulo.data$omega[i]*cos(pendulo.data$omega[i]*t)))
}
pendulo.Ecin <- function(t,i)
{
vel <- pendulo.Velocidade(t,i)
return((pendulo.massa*(vel^2))/2)
}
pendulo.Epot <- function(t,i)
{
var_theta <- pendulo.theta*cos(pendulo.data$omega[i]*t)
return(pendulo.massa*G*pendulo.data$comprimento[i]*(1-cos(var_theta)))
}
pendulo.lm <- lm(pendulo.data$tmedia2 ~ pendulo.data$comprimento)
